{
  "name": "mesiu.github.io",
  "version": "1.0.0",
  "description": "My personal website",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mesiu/mesiu.github.io.git"
  },
  "author": "Melissa Siu",
  "scripts": {
    "compile": "harp compile src dist",
    "deploy": "npm run compile && npm run ghpages",
    "ghpages": "git-directory-deploy --directory dist --branch master",
    "start": "harp server src"
  },
  "devDependencies": {
    "express": "4.14.0",
    "git-directory-deploy": "1.5.1",
    "harp": "0.23.0"
  }
}
